CREATE TABLE phones (
    model VARCHAR(10)
    , serviceCompany VARCHAR(5)
    , PRIMARY KEY (model, serviceCompany)
);
DESC phones;
-- DROP TABLE phones;
INSERT INTO phones (model, serviceCompany) VALUES ("S20", "KT");
